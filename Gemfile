# Gemfile ― built for GitHub Pages

source "https://rubygems.org"

# The one-stop meta-gem that pins exactly the Jekyll version and
# the whitelisted plugins (jekyll-feed, jekyll-sitemap, jekyll-seo-tag, …)
# used by GitHub Pages’ own build system.
gem "github-pages", group: :jekyll_plugins

# Optional -- makes `bundle exec jekyll serve` work locally on Ruby 3.x
gem "webrick", "~> 1.8", platforms: [:ruby]
